<!-- 
xmlns:lock="http://blog.codealy.com/schema/lock" 是用来指定自定义schema
xsi:schemaLocation用来指定xsd文件
 -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context" 
	xmlns:codealy="http://blog.codealy.com/schema/codealy"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
		http://blog.codealy.com/schema/codealy http://blog.codealy.com/schema/codealy/codealy.xsd">

	<codealy:lock id="lockMe" key="lockDemo" name="lockDemo1" group="lockDemoGroup"/>
	<codealy:lock id="lockYou" key="lockYou" name="lockYou" group="lockDemoGroup"/>

	<!--
	<codealy:kapo interface="sss"/>
	-->
</beans>